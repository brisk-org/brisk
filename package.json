{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "watch": "tsc --watch",
    "build": "tsc && yarn generate && yarn migration ",
    "service": "node service.js",
    "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration -p",
    "migration": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop"
  },
  "dependencies": {
    "@types/express-fileupload": "^1.2.2",
    "@types/graphql-upload": "^8.0.10",
    "@types/multer": "^1.4.7",
    "apollo-server-express": "^2.24.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "connect-pg-simple": "^6.2.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "graphql": "^15.5.0",
    "graphql-upload": "^13.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "mysql": "^2.14.1",
    "node-windows": "^1.0.0-beta.5",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.10",
    "type-graphql": "^1.1.1",
    "dotenv": "^9.0.2",
    "typeorm": "0.2.32"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^8.0.29",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
